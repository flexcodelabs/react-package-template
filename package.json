  "description": "",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "clean": "rimraf lib/",
    "copy-esm-files": "copyfiles -u 1 src/**/*.css lib/esm",
